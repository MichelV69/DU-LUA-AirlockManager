{
  "cli": {
    "fmtVersion": 4
  },
  "name": "AirlockManager",
  "description": "LUA Script to produce 'Realistic' / RP-friendly behaviour for 'Airlocks'",
  "sourcePath": "src",
  "outputPath": "out",
  "libs": [],
  "builds": {
    "mainScript": {
      "name": "main",
      "type": "control",
      "slots": {}
    },
    "fixedSlots": {
      "name": "mainWFS",
      "type": "control",
      "slots": {
        "0": {
            "name": "MyScreen",
            "type": "screen"
        },
        "1": {
            "name": "MyDB",
            "type": "databank"
        }
      }
    }
  },
  "targets": {
    "development": {
      "name": "development",
      "minify": false,
      "handleErrors": false
    },
    "release": {
      "name": "release",
      "minify": true,
      "handleErrors": false
    }
  },
  "internalPaths": [
    "autoconf/",
    "cpml/",
    "pl/",
    "utils/event"
  ]
}
