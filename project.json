{
  "cli": {
    "fmtVersion": 4
  },
  "name": "AirlockManager",
  "description": "LUA Script to produce 'Realistic' / RP-friendly behaviour for 'Airlocks'",
  "sourcePath": "src",
  "outputPath": "out",
  "libs": [
    {
      "id": "du-mocks",
      "type": "raw",
      "path": "../_templates/du-mocks/src/du-mocks"
    }
  ],
  "builds": {
    "mainWFS": {
      "name": "mainWFS",
      "type": "control",
      "slots": {
        "0": {
          "name": "insideSwitch",
          "type": "ManualSwitchUnit"
        },
        "1": {
          "name": "insideDoor",
          "type": "DoorUnit"
        },
        "2": {
          "name": "outsideSwitch",
          "type": "ManualSwitchUnit"
        },
        "3": {
          "name": "outsideDoor",
          "type": "DoorUnit"
        },
        "4": {
          "name": "insideLight",
          "type": "LightUnit"
        },
        "5": {
          "name": "outsideLight",
          "type": "LightUnit"
        },
        "6": {
          "name": "middleLight",
          "type": "LightUnit"
        },
        "7": {
          "name": "insideSwitchLight",
          "type": "LightUnit"
        },
        "8": {
          "name": "outsideSwitchLight",
          "type": "LightUnit"
        },
        "9": {
          "name": "statusScreen1",
          "type": "ScreenUnit"
        },
        "10": {
          "name": "statusScreen1",
          "type": "ScreenUnit"
        }
      }
    }
  },
  "targets": {
    "development": {
      "name": "development",
      "minify": false,
      "handleErrors": false
    },
    "release": {
      "name": "release",
      "minify": true,
      "handleErrors": false
    }
  },
  "internalPaths": [
    "autoconf/",
    "cpml/",
    "pl/",
    "utils/event"
  ]
}